{
  "name": "maatify/rate-limiter",
  "description": "PSR-compliant rate limiter with Redis, MongoDB, and MySQL support.",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Maatify\\RateLimiter\\": "src/"
    }
  },
  "require": {
    "php": ">=8.4",
    "ext-redis": "*",
    "ext-mongodb": "*",
    "ext-pdo_mysql": "*",
    "maatify/psr-logger": "^1.0",
    "mongodb/mongodb": "^2.0",
    "ext-pdo": "*",
    "psr/http-message": "^2.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
